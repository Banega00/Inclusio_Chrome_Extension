{
  "name": "Inclusion extension",
  "version": "0.1.0",
  "description": "Inclusion extension",
  "permissions": [
    "storage",
    "tabs",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_script.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "icons": {
  },
  "action": {
    "default_icon": {
    },
    "default_title": "Inclusion extension",
    "default_popup": "popup.html"
  },
  "manifest_version": 3
}