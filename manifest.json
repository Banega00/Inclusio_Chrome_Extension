{
  "name": "Inclusion extension",
  "version": "0.1.0",
  "description": "Inclusion extension",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "unlimitedStorage"
  ],
  "options_ui": {
    "page": "options.html",
    "browser_style": true,
    "open_in_tab": true
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content_script.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "icons": {
    "64": "assets/logo.png"
  },
  "action": {
    "default_icon": {
    },
    "default_title": "Inclusion extension",
    "default_popup": "popup.html"
  },
  "manifest_version": 3
}